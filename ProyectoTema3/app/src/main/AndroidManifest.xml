        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoTema3"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity4"
            android:exported="false" />
        <activity
            android:name=".MainActivity3"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
